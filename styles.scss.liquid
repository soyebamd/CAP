/****************************************************************************** 
*********************************** COLORS ************************************
******************************************************************************/

$white:       #FFFFFF;
$shadow-grey: #dddddd;
$off-black:   #010101;
$black:       #000000;

$testing:     #FF0000;
@font-face {
  font-family: moonb;
  src: url({{ 'Moonb.ttf' | asset_url }});
}
@font-face {
  font-family: MoonFlower;
  src: url({{ 'Moon Flower.ttf' | asset_url }});
}

@font-face {
  font-family: bebasbold;
  src: url({{ 'BebasNeue_Bold.ttf' | asset_url }});
}

@font-face {
  font-family: bebasregular;
  src: url({{ 'BebasNeue_Regular.ttf' | asset_url }});
}

@font-face {
  font-family: bebasbook;
  src: url({{ 'BebasNeue_Book.ttf' | asset_url }});
}
/****************************************************************************** 
******************************** BODY ELEMENTS ********************************
******************************************************************************/
html, body{
  height: 100%;
  font-family: $archer-book;
}
*{
  font-family: $archer-book;
}

#wrap{
  min-height: 100%;
  width: 100%;
  overflow-x: hidden;
  #main{
    overflow-y:auto;
    overflow-x:hidden;
    padding-bottom: 250px;
  }
}
.header-space{
  height: 85px;
}

.action_link{
  padding: 5px 8px;
  a{
    color: $black;
  }
}

del{
  color: #FF0000;
}



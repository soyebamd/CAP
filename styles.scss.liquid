/****************************************************************************** 
*********************************** FONTS *************************************
******************************************************************************/

$gill-sans:   'cooper_hewittmedium', Futura, "Trebuchet MS", Arial, sans-serif;
$archer-book: 'cooper_hewittmedium',  Futura, "Trebuchet MS", Arial, sans-serif;

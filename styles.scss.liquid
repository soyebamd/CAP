/****************************************************************************** 
********************************* TYPOGRAPHY **********************************
******************************************************************************/
a, p, span, ul, ol, li{
  font-weight:100;  
  text-decoration: none;
}

a:hover {
  background-repeat:no-repeat;
  background-position:middle;
  color:#ccc;
}


.product-loop-item .details a {}
/****************************************************************************** 
*********************************** FOOTER ************************************
******************************************************************************/
#footer{
  position: relative;
  margin-top: -250px;
  padding-top: 15px;
  height: 250px;
  clear:both;
  font-family:'cooper_hewittmedium';
  background: $black;
  color: $white;
  h5{
    color: inherit;
    margin-bottom: 5px;
  }
  .footer-nav{
    li{
      float:none;
      font-size: 10px;
      margin:0px;
      a{
        color: $white;
        text-transform: uppercase;
        font-family:'cooper_hewittmedium';
      }
    }
  }
  #back-to-top{
    width: 81px;
    height: 21px;
    margin-top: 9px;
    float: right;
  }
  #footer-social{
    li{
      float: left;
      margin-right: 30px;
    }
  }
  #footer-emailicon{
    float: left;
    padding: 5px 10px 0 0;
  }
  #footer-newsletter{
    text-align:left;
    .mc-field-group{
      text-align:left;
      @media only screen and (max-width: 767px){
        text-align:center;
      }
    }
    input[type='text']{
      font-family: $archer-book;
      text-transform: uppercase;
      
    }
    input[type='submit'], button[type='submit']{
      
      /*background: url('img-little-arrow.png') no-repeat;*/
      margin: 11px 0 0 -21px;
      text-shadow: none;
      /*color: transparent !important; */	
      color:#fff !important;
      padding:4px 6px;
      margin-top:14px;
      text-transform:uppercase;
      font-weight:100;
      font-family:'cooper_hewittmedium';
      background-color:#000;
      border:0px;

    }
  }
  ul.bottom{
    color: $white;
    li{
      display: inline-block;
      padding: 0px 0px 0px 10px;
      a{
        color: inherit;
        font-size: 12px;
      }
    }
  }
  ul#bottom-right{
    color: $white;
    float: right;
    li{
      float: left;
      padding: 0px 0px 0px 10px;
      a{
        color: inherit;
        font-size: 12px;
      }
    }
  }
} 

/****************************************************************************** 
****************************** NAVIGATION MENUS *******************************
******************************************************************************/

#site-header{
  width: 100%;
  position: fixed;
  z-index: 9999;
  height:85px;
  #menu-container{	
    width: 100%;
    background: $white;
    .container{
      height: 85px;
      #logo > img{
        height: 85px;
        width: 148px;
      }
      #left-nav{
        margin-top: 15px;
        li{
          float: left;
          height: 50px;
          padding: 0 18px;
          padding-left: 0px;
          text-transform: uppercase;
          font-size: 15px;
          .sub-nav{ display:none;margin: 0px;position: absolute;top: 49px;width:150px;
            padding: 5px; z-index:10000;background:#fff;}
          .sub-nav li{ float:left; margin:0px; height: 30px;}
          a{
            height: 100%;
            line-height: 50px;
            font-family: 'cooper_hewittmedium';
            font-size: 14px;
          }
          &:hover .sub-nav{
            display:block;
          }   
          .sub-nav li a{ line-height:30px;}
        }
        li:last-child{
          padding-right:0px;
        }
      }
      #right-nav{
        float: right;
        margin-top: 15px;
        li{
          float: left;
          height: 50px;
          padding: 0 0px;
          text-transform: uppercase;
          .sub-nav{ display:none;margin: 0px;
            position: absolute;top: 49px;width:100px;
            padding: 5px; z-index:10000;background:#fff;
          left:0px;}
          .sub-nav li{ float:left; margin:0px; height: 30px;}
          a{
            height: 100%;
            line-height: 50px;	
            font-family: 'cooper_hewittmedium';
            font-size: 14px;
            color: #000;
            
          }          
          &:hover .sub-nav{
            display:block;
          }   
          .sub-nav li a{ line-height:30px;}
        }
        li:last-child{ padding-right:0px;}	
      }
    }
    .alpha{
      margin-right:20px;
    }
    .omega{
      margin-left:20px;
    }
  }
}
#site-mobile-header{
  display: none;
}

/****************************************************************************** 
********************************** HOME PAGE **********************************
******************************************************************************/
#home{
  #home-slider-container{
    width: 100%;
    margin:0 auto;
    margin-bottom:30px;
    .bxslider{
      width: 100%;
      li{
        width: 100%;
        img{
          min-height: 0px !important;
        }
      }
    }
  }
  #home-carousel-container{
    margin-bottom: -10px !important;
    #carousel-hover{
      width: 245px;
      position: relative;
    }
    ul{
      li{
        a{
          .hover-title{
            position: absolute;
            color: white;
            font-family:'cooper_hewittmedium';
            text-align: center;
            width: 100%;
            z-index: 99999;
            margin-top: -130px;
            font-size: 24px;

          }
          .hover-readmore{
            position: absolute;
            color: white;
            text-align: center;
            width: 100%;
            z-index: 99999;
            margin-top: -120px;
            font-size: 16px;
            text-shadow: 1px 1px 2px $black,
              1px 2px 1px $black;												
          }
        }
        background:#000;
      }
    }
  }
  #home-bottom-info{
    .one-third{
      border-bottom: 2px $black solid;
      margin-bottom: 50px;
      min-height: 325px;
      img{
        max-width: 100%;
        display: block;
        margin: 0 auto;
      }
      ul{
        margin: 20px 0 0 0;
        li{
          width: 80%;
          a{
            font-weight: 100;

            img{
              float: right;
            }
          }

        }
      }
    }
  }
  .blogrss{
    display:none;  
  }
  .blogrss .one-third ul li{
    float: none !important;
    width: auto !important;
  }    
  #sliding-panes{
    margin-top:30px;
    height: 600px;
    margin-bottom: 30px;
    position: relative;
    overflow: hidden;
    .rail-details{
      position: absolute;			
      color: $white;
      h4{
        text-transform: uppercase;
        font-family: $archer-book;
        color: $white;
      }
      h1{
        text-transform: uppercase;
        font-family: $gill-sans;
        color: $white;
